{
  "name": "@ssdcode/vue-event-bus",
  "version": "0.1.8",
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastiansulinski/vue-event-bus.git"
  },
  "homepage": "https://github.com/sebastiansulinski/vue-event-bus#readme",
  "main": "./dist/vue-event-bus.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
